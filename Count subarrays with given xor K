class Solution:
    def subarraysWithXorK(self, nums, k):
        n = len(nums)
        cnt = 0
        for i in range(n):
            for j in range(i, n):
                xorr = 0
                for K in range(i, j + 1):
                    xorr ^= nums[K]
                if xorr == k:
                    cnt += 1
        return cnt
