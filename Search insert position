class Solution:
    def searchInsert(self, nums, target):
        for i in range(len(nums)):
            if nums[i] == target:
                return i
            else :
                ans = len(nums)
                low = 0
                high = len(nums)-1
                while low <=high:
                    mid = (high+low)//2
                    if nums[mid] >= target:
                        ans = mid
                        high = mid -1
                    else :
                        low = mid+1 
            return ans
                         
