class Solution:
    def findKRotation(self, nums):
        low, high = 0, len(nums) - 1
        ans = float('inf')
        index = -1
        while low <= high:
            mid = (low + high) // 2
            if nums[low] <= nums[high]:
                if nums[low] < ans:
                    index = low
                    ans = nums[low]
                break
            if nums[low] <= nums[mid]:
                if nums[low] < ans:
                    index = low
                    ans = nums[low]
                low = mid + 1
            else:
                if nums[mid] < ans:
                    index = mid
                    ans = nums[mid]
                high = mid - 1
        return index
