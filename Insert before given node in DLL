class ListNode:
    def __init__(self, data1=0, next1=None, prev1=None):
        self.val = data1
        self.next = next1
        self.prev = prev1

class Solution:
    def insertBeforeGivenNode(self, node, X):
        prev = node.prev
        newNode = ListNode(X, node, prev)
        prev.next = newNode
        node.prev = newNode
        return

def build_list(arr):
    head = None
    trav = None
    for val in arr:
        temp = ListNode(val)
        if head is None:
            head = temp
            trav = temp
        else:
            trav.next = temp
            temp.prev = trav
            trav = trav.next
    return head
