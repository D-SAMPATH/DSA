class Solution:
    def deleteHead(self, head):
        if head is None:
            return None  
        temp = head
        head = head.next
        del temp  
        return head

class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def build_list(arr):
    head = None
    trav = None
    for val in arr:
        temp = ListNode(val)
        if not head:
            head = temp
            trav = temp
        else:
            trav.next = temp
            trav = trav.next
    return head
