class Solution:
    def rotateArrayByOne(self, nums):
        temp = nums[0]
        for i in range(1, len(nums)):
            nums[i - 1] = nums[i]
        nums[-1] = temp

//can be done in another way 

class Solution:
    def rotateArrayByOne(self, nums):
        for i in range(0,1):
            nums.append(nums[i])
            nums.pop(i)
        return nums
