class Solution:
    def generateRow(self, row):
        ans = 1
        ansRow = []
        ansRow.append(1)
        for col in range(1, row):
            ans = ans * (row - col)
            ans = ans // col
            ansRow.append(ans)
        return ansRow  
    def pascalTriangleIII(self, n):
        pascalTriangle = []
        for row in range(1, n + 1):
            pascalTriangle.append(self.generateRow(row))
        return pascalTriangle
