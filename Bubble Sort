class Solution:
    def bubbleSort(self, nums):
        n=len(nums)
        for i in range(0,len(nums)):
            swap=False
            for j in range(0,len(nums)-i-1):
                    if nums[j]>nums[j+1]:
                        nums[j], nums[j+1] = nums[j+1] , nums[j]
                        swap=True
            
            if not swap:
                break
        return nums

