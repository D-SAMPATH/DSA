class Solution:
    def threeSum(self, nums: List[int]) -> List[List[int]]:
        triplet_set = set()
        n=len(nums)
        for i in range(n):
            hasmap = set()
            for j in range(i+1,n):
                third = -(nums[i] + nums[j])
                if third in hasmap :
                    temp = [nums[i] , nums[j] ,third]
                    temp.sort()
                    triplet_set.add(tuple(temp))
                hasmap.add(nums[j])
        ans = [list(i) for i in triplet_set]
        return ans
