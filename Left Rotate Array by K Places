class Solution:
    def rotateArray(self, nums, k):
        for i in range(0,k):
            nums.append(nums[i-i])
            nums.pop(i-i)
        return nums

//another way to solve 
class Solution:
    def rotateArray(self, nums, k):
        n = len(nums)
        k = k % n  
        temp = []
        for i in range(k):
            temp.append(nums[i])
        for i in range(k, n):
            nums[i - k] = nums[i]
        for i in range(k):
            nums[n - k + i] = temp[i]
